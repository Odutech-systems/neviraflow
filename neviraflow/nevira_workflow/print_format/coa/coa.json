 "html": "{% macro add_letter_head(letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n{%- endmacro %}\r\n\r\n{{ add_letter_head(letter_head, no_letterhead) }}\r\n\r\n<div class=\"header\">\r\n\r\n    <div style=\"text-align: center; margin-top: 20px;\">\r\n        <h3 style=\"margin-top: 10px;\">Certificate of Analysis</h3>\r\n    </div>\r\n</div>\r\n\r\n<div class=\"content\">\r\n    <div style=\"display: flex; justify-content: space-between; margin-top: 20px;\">\r\n        <div style=\"width: 48%;\">\r\n            <p><strong>Product:</strong> {{ doc.product }}</p>\r\n             <p><strong>Product Name:</strong> {{ doc.custom_product_name }}</p>\r\n            <p><strong>Customer:</strong> {{ doc.custom_customer_name }}</p>\r\n            <p><strong>Quantity:</strong> {{ doc.custom_batch_quantity }} {{ doc.custom_uom }}</p>\r\n            <p><strong>Batch No.:</strong> {{ doc.custom_batch_number }}</p>\r\n        </div>\r\n        <div style=\"width: 48%;\">\r\n            <p><strong>Analysis Date:</strong> {{ doc.production_date }}</p>\r\n            <p><strong>Production Date:</strong> {{ doc.production_date }}</p>\r\n            <p><strong>Expiry Date:</strong> {{ doc.expiry_date }}</p>\r\n            <p><strong>Invoice No.:</strong> {{ doc.invoice_number }}</p>\r\n            <p><strong>Date:</strong> {{ frappe.utils.formatdate(doc.creation) }}</p>\r\n        </div>\r\n    </div>\r\n\r\n    <hr style=\"margin-top: 20px; border: 0; border-top: 1px solid #ccc;\">\r\n\r\n    <p style=\"margin-top: 20px;\"><strong>Test Results:</strong></p>\r\n    <p>This is to certify that the {{ doc.custom_product_name }} conforms to the following test results.</p>\r\n\r\n    <table style=\"width: 100%; border-collapse: collapse; margin-top: 20px;\">\r\n        <thead>\r\n            <tr style=\"background-color: #f2f2f2;\">\r\n                <th style=\"border: 1px solid #ccc; padding: 8px; text-align: left;\">Parameters</th>\r\n                <th style=\"border: 1px solid #ccc; padding: 8px; text-align: left;\">Test Results (%)</th>\r\n                <th style=\"border: 1px solid #ccc; padding: 8px; text-align: left;\">Specifications (%)</th>\r\n            </tr>\r\n        </thead>\r\n        <tbody>\r\n            {% for item in doc.params_table %}\r\n            <tr>\r\n                <td style=\"border: 1px solid #ccc; padding: 8px;\">{{ item.test_parameter }}</td>\r\n                <td style=\"border: 1px solid #ccc; padding: 8px;\">{{ item.analysis }}</td>\r\n                <td style=\"border: 1px solid #ccc; padding: 8px;\">{{ item.specification }}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </tbody>\r\n    </table>\r\n\r\n    <div style=\"margin-top: 50px;\">\r\n        <p>Analysed By: {{ doc.analysed_by }}</p>\r\n        <div style=\"margin-top: 10px;\">\r\n            <p>_ _ _ _ _ _ _ _ _ _ _ _ _</p>\r\n            <p>Certificate Of Analysis</p>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<style>\r\n    body {\r\n        font-family: Arial, sans-serif;\r\n    }\r\n    .header, .content {\r\n        padding: 0 20px;\r\n    }\r\n    .header {\r\n        border-bottom: 2px solid #000;\r\n        padding-bottom: 10px;\r\n    }\r\n    p {\r\n        margin: 5px 0;\r\n    }\r\n</style>",
